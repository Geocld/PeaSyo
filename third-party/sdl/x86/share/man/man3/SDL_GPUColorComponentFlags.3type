.BI "#define SDL_GPU_COLORCOMPONENT_R (1u << 0) /**< the red component */
.BI "#define SDL_GPU_COLORCOMPONENT_G (1u << 1) /**< the green component */
.BI "#define SDL_GPU_COLORCOMPONENT_B (1u << 2) /**< the blue component */
.BI "#define SDL_GPU_COLORCOMPONENT_A (1u << 3) /**< the alpha component */
