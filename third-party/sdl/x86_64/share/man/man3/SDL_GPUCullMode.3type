.SH NAME
SDL_GPUCullMode \- Specifies the facing direction in which triangle faces will be culled\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_gpu\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_GPUCullMode
.BI "{
.BI "    SDL_GPU_CULLMODE_NONE,   /**< No triangles are culled. */
.BI "    SDL_GPU_CULLMODE_FRONT,  /**< Front-facing triangles are culled. */
.BI "    SDL_GPU_CULLMODE_BACK    /**< Back-facing triangles are culled. */
.BI "} SDL_GPUCullMode;
.fi
