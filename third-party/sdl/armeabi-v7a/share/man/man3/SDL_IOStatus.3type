.TH SDL_IOStatus 3type "SDL 3.1.3" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_IOStatus \- 
.BR SDL_IOStream
 status, set by a read or write operation\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_iostream\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_IOStatus
.BI "{
.BI "    SDL_IO_STATUS_READY,     /**< Everything is ready (no errors and not EOF). */
.BI "    SDL_IO_STATUS_ERROR,     /**< Read or write I/O error */
.BI "    SDL_IO_STATUS_EOF,       /**< End of file */
.BI "    SDL_IO_STATUS_NOT_READY, /**< Non blocking I/O, not ready */
.BI "    SDL_IO_STATUS_READONLY,  /**< Tried to write a read-only buffer */
.BI "    SDL_IO_STATUS_WRITEONLY  /**< Tried to read a write-only buffer */
.BI "} SDL_IOStatus;
.fi
.SH AVAILABILITY
