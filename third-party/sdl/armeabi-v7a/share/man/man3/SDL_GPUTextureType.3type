.TH SDL_GPUTextureType 3type "SDL 3.1.3" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_GPUTextureType \- Specifies the type of a texture\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_gpu\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_GPUTextureType
.BI "{
.BI "    SDL_GPU_TEXTURETYPE_2D,         /**< The texture is a 2-dimensional image. */
.BI "    SDL_GPU_TEXTURETYPE_2D_ARRAY,   /**< The texture is a 2-dimensional array image. */
.BI "    SDL_GPU_TEXTURETYPE_3D,         /**< The texture is a 3-dimensional image. */
.BI "    SDL_GPU_TEXTURETYPE_CUBE,       /**< The texture is a cube image. */
.BI "    SDL_GPU_TEXTURETYPE_CUBE_ARRAY  /**< The texture is a cube array image. */
.BI "} SDL_GPUTextureType;
.fi
