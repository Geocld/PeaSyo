.TH SDL_GPUVertexElementFormat 3type "SDL 3.1.3" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_GPUVertexElementFormat \- Specifies the format of a vertex attribute\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_gpu\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef enum SDL_GPUVertexElementFormat
.BI "{
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_INVALID,
.BI "
.BI "    /* 32-bit Signed Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_INT,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_INT2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_INT3,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_INT4,
.BI "
.BI "    /* 32-bit Unsigned Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UINT,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UINT2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UINT3,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UINT4,
.BI "
.BI "    /* 32-bit Floats */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_FLOAT,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_FLOAT2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_FLOAT3,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_FLOAT4,
.BI "
.BI "    /* 8-bit Signed Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_BYTE2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_BYTE4,
.BI "
.BI "    /* 8-bit Unsigned Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UBYTE2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UBYTE4,
.BI "
.BI "    /* 8-bit Signed Normalized */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_BYTE2_NORM,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_BYTE4_NORM,
.BI "
.BI "    /* 8-bit Unsigned Normalized */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UBYTE2_NORM,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_UBYTE4_NORM,
.BI "
.BI "    /* 16-bit Signed Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_SHORT2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_SHORT4,
.BI "
.BI "    /* 16-bit Unsigned Integers */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_USHORT2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_USHORT4,
.BI "
.BI "    /* 16-bit Signed Normalized */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_SHORT2_NORM,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_SHORT4_NORM,
.BI "
.BI "    /* 16-bit Unsigned Normalized */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_USHORT2_NORM,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_USHORT4_NORM,
.BI "
.BI "    /* 16-bit Floats */
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_HALF2,
.BI "    SDL_GPU_VERTEXELEMENTFORMAT_HALF4
.BI "} SDL_GPUVertexElementFormat;
