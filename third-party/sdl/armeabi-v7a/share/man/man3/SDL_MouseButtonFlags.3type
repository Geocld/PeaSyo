.TH SDL_MouseButtonFlags 3type "SDL 3.1.3" "Simple Directmedia Layer" "SDL3 DATATYPES"
.SH NAME
SDL_MouseButtonFlags \- A bitmask of pressed mouse buttons, as reported by 
.BR SDL_GetMouseState
, etc\[char46]
.SH HEADER FILE
Defined in SDL3/SDL_mouse\[char46]h

.SH SYNOPSIS
.nf
.B #include \(dqSDL3/SDL.h\(dq
.PP
.BI "typedef Uint32 SDL_MouseButtonFlags;
.BI "
.BI "#define SDL_BUTTON_LEFT     1
.BI "#define SDL_BUTTON_MIDDLE   2
.BI "#define SDL_BUTTON_RIGHT    3
.BI "#define SDL_BUTTON_X1       4
.BI "#define SDL_BUTTON_X2       5
.BI "
.BI "#define SDL_BUTTON_MASK(X)  (1u << ((X)-1))
.BI "#define SDL_BUTTON_LMASK    SDL_BUTTON_MASK(SDL_BUTTON_LEFT)
.BI "#define SDL_BUTTON_MMASK    SDL_BUTTON_MASK(SDL_BUTTON_MIDDLE)
.BI "#define SDL_BUTTON_RMASK    SDL_BUTTON_MASK(SDL_BUTTON_RIGHT)
.BI "#define SDL_BUTTON_X1MASK   SDL_BUTTON_MASK(SDL_BUTTON_X1)
.BI "#define SDL_BUTTON_X2MASK   SDL_BUTTON_MASK(SDL_BUTTON_X2)
.fi
.SH DESCRIPTION

\(bu Button 1: Left mouse button

\(bu Button 2: Middle mouse button

\(bu Button 3: Right mouse button

\(bu Button 4: Side mouse button 1

\(bu Button 5: Side mouse button 2

.SH AVAILABILITY
This datatype is available since SDL 3\[char46]0\[char46]0\[char46]

